<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>ticketline.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>ticketline.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>ticketline.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MasterFragment</managed-bean-name>
        <managed-bean-class>ticketline.MasterFragment</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MainPage</managed-bean-name>
        <managed-bean-class>ticketline.MainPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Top10</managed-bean-name>
        <managed-bean-class>ticketline.Top10</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ManageAccount</managed-bean-name>
        <managed-bean-class>ticketline.ManageAccount</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Products</managed-bean-name>
        <managed-bean-class>ticketline.Products</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Kassa</managed-bean-name>
        <managed-bean-class>ticketline.Kassa</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SearchResultVeranstaltungen</managed-bean-name>
        <managed-bean-class>ticketline.SearchResultVeranstaltungen</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PlatzAuswahl</managed-bean-name>
        <managed-bean-class>ticketline.PlatzAuswahl</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SearchResult</managed-bean-name>
        <managed-bean-class>ticketline.SearchResult</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SearchResultAuffuehrungen</managed-bean-name>
        <managed-bean-class>ticketline.SearchResultAuffuehrungen</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Warenkorb</managed-bean-name>
        <managed-bean-class>ticketline.Warenkorb</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Booking</managed-bean-name>
        <managed-bean-class>ticketline.Booking</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/SearchResult.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Booking.jsp</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Warenkorb.jsp</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>buy</from-outcome>
            <to-view-id>/Kassa.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Kassa.jsp</from-view-id>
        <navigation-case>
            <from-outcome>finish</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ManageAccount.jsp</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>saved</from-outcome>
            <to-view-id>/MainPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/SearchResultVeranstaltungen.jsp</from-view-id>
        <navigation-case>
            <from-outcome>select</from-outcome>
            <to-view-id>/SearchResultAuffuehrungen.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/SearchResult.jsp</from-view-id>
        <navigation-case>
            <from-outcome>select</from-outcome>
            <to-view-id>/SearchResultAuffuehrungen.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>selectort</from-outcome>
            <to-view-id>/SearchResultVeranstaltungen.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>selectkuenstler</from-outcome>
            <to-view-id>/SearchResultVeranstaltungen.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Products.jsp</from-view-id>
        <navigation-case>
            <from-outcome>select</from-outcome>
            <to-view-id>/Warenkorb.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Top10.jsp</from-view-id>
        <navigation-case>
            <from-outcome>select</from-outcome>
            <to-view-id>/SearchResultAuffuehrungen.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/SearchResultAuffuehrungen.jsp</from-view-id>
        <navigation-case>
            <from-outcome>select</from-outcome>
            <to-view-id>/PlatzAuswahl.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PlatzAuswahl.jsp</from-view-id>
        <navigation-case>
            <from-outcome>buy</from-outcome>
            <to-view-id>/Kassa.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>book</from-outcome>
            <to-view-id>/Booking.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>VeranstaltungsFragment</managed-bean-name>
        <managed-bean-class>ticketline.VeranstaltungsFragment</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
